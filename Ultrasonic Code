#pragma config(Sensor, dgtl1,  sonar,          sensorSONAR_inch)
#pragma config(Motor,  port10,          tester,        tmotorVex393_HBridge, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//


task main()
{
	repeat (forever) {
		joystickControl(port10, Ch2, 10);
		if (SensorValue[sonar] <= 2) {
			if (vexRT[Ch2] >= 1) {
				stopMotor(port10);
			}
		}
	}
}
